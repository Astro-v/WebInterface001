<!DOCTYPE html>
<html>

<head>
    <title>Directories</title>
    <link rel="stylesheet" type="text/css" href="/directories_styles.css">
</head>

<body>
    <header>
        <p>
            <%= curent_path %>
        </p>
    </header>
    <nav>
        <div class="icon">
            <img src="/images/icon/account_circle.png" alt="home">
        </div>
    </nav>
    <div class="container">
        <% directories.forEach(function(directory) { %>
            <a href="/directories<%= curent_path %>/<%= directory %>">
                <div class="item">
                    <%= directory %>
                </div>
            </a>
            <% }); %>
    </div>
</body>

</html>